#include<iostream>
using namespace std;
int main(){
    int n,r;
    int a,b;
  
  
    int sum=0;
    cin>>n;
    b=n;
    while(n>0){
        int i;
        i=n%10;
        a=(i* i* i);
        sum=sum+a;
        
        n=n/10;
    }
    cout<<sum<<endl;
    
    if(b==sum){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"no"<<endl;
    }
    
    
    
    
    
}

    
    
