 void pattern9(int n) {
        // code here
        for(int i=1;i<=2*n-1;i++){
            int stars;
            if(i>n){
                stars=2*n-i;
            }
            else{
                stars=i;
            }
            for(int j=0;j<stars;j++){
                cout<<"* ";
            }
            cout<<endl;
        }
    }
